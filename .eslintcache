.js",["55"],"/home/zubair/redux-shop/src/store/reducers/ProductsReducer.js",[],"/home/zubair/redux-shop/src/store/index.js",[],"/home/zubair/redux-shop/src/components/Details.js",["56","57"],"import React, { useEffect, useState } from \"react\";\nimport { useParams } from \"react-router-dom\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport currencyFormatter from \"currency-formatter\";\nimport { BsDash, BsPlus } from \"react-icons/bs\";\n\nconst Details = () => {\n  const [quantity, setQuantity] = useState(1);\n  const { id } = useParams();\n  const dispatch = useDispatch();\n\n  const { product } = useSelector((state) => state.ProductsReducer);\n  useEffect(() => {\n    dispatch({ type: \"PRODUCT\", id });\n  }, [id]);\n\n  const decQuantity = () => {\n    if (quantity > 1) {\n      setQuantity(quantity - 1);\n    }\n  };\n\n  return (\n    <div className=\"container mt-100\">\n      <div className=\"row2\">\n        <div className=\"col-6\">\n          <div className=\"details__image\">\n            {product.image ? (\n              <img src={require(`../assets/${product.image}`).default} />\n            ) : null}\n          </div>\n        </div>\n        <div className=\"col6-6 \">\n          <div className=\"details__name\">{product.name}</div>\n          <div className=\"details__prices\">\n            <span className=\"details__actual\">\n              {currencyFormatter.format(product.price, {\n                code: \"USD\",\n              })}\n            </span>\n            <span className=\"details__discount\">\n              {currencyFormatter.format(product.discountPrice, {\n                code: \"USD\",\n              })}\n            </span>\n          </div>\n          <div className=\"details__info\">\n            <div className=\"details__incdec\">\n              <span className=\"dec\" onClick={decQuantity}>\n                <BsDash />\n              </span>\n              <span className=\"quantity\">{quantity}</span>\n              <span className=\"inc\" onClick={() => setQuantity(quantity + 1)}>\n                <BsPlus />\n              </span>\n              <button\n                className=\"btn-default\"\n                onClick={() =>\n                  dispatch({\n                    type: \"ADD_TO_CART\",\n                    payload: { product, quantity },\n                  })\n                }\n              >\n                Add to cart\n              </button>\n            </div>\n          </div>\n\n          <div className=\"details__p\">\n            <h4>Details</h4>\n            {product.desc}\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default Details;\n","/home/zubair/redux-shop/src/store/reducers/CartReducer.js",[],{"ruleId":"58","severity":1,"message":"59","line":2,"column":8,"nodeType":"60","messageId":"61","endLine":2,"endColumn":14},{"ruleId":"62","replacedBy":"63"},{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","severity":1,"message":"67","line":15,"column":15,"nodeType":"68","endLine":15,"endColumn":33},{"ruleId":"66","severity":1,"message":"67","line":20,"column":21,"nodeType":"68","endLine":20,"endColumn":80},{"ruleId":"66","severity":1,"message":"67","line":35,"column":25,"nodeType":"68","endLine":37,"endColumn":27},{"ruleId":"69","severity":1,"message":"70","line":15,"column":6,"nodeType":"71","endLine":15,"endColumn":10,"suggestions":"72"},{"ruleId":"66","severity":1,"message":"67","line":29,"column":15,"nodeType":"68","endLine":29,"endColumn":74},"no-unused-vars","'Header' is defined but never used.","Identifier","unusedVar","no-native-reassign",["73"],"no-negated-in-lhs",["74"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'dispatch'. Either include it or remove the dependency array.","ArrayExpression",["75"],"no-global-assign","no-